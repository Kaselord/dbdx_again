; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="dbdx_again"
config/features=PackedStringArray("4.2", "GL Compatibility")
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/show_image=false
config/icon="res://icon.svg"

[debug]

shapes/collision/shape_color=Color(1, 1, 1, 0.42)

[display]

window/size/viewport_width=640
window/size/viewport_height=384
window/size/window_width_override=640
window/size/window_height_override=384
window/stretch/mode="viewport"

[editor]

naming/node_name_casing=2

[editor_plugins]

enabled=PackedStringArray("res://addons/cheesegodot/plugin.cfg")

[filesystem]

import/blender/enabled=false

[rendering]

textures/canvas_textures/default_texture_filter=0
renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
